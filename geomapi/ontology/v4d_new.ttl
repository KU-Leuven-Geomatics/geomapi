						owl:versionIRI <https://w3id.org/geomapi-0.0.1> ;
						dcterms:title    "Geomatics API ontology (GEOMAPI)"@en ;
						dcterms:description    """The Geomatics API ontology (GEOMAPI) is an ontology defining the core concepts of close-range sensing observations.
						It manage point clouds, imagery, polygonal meshes, BIM and CAD information in relation with the geomapi api (https://github.com/KU-Leuven-Geomatics/geomapi).
						dcterms:creator "Maarten Bassier" ;
						dcterms:creator "Jelle Vermandere" ;
						dcterms:creator "Heinder De Winter" ;
						dcterms:creator "Sam De Geyter" ;
						dcterms:creator <https://orcid.org/0000-0002-5231-2853> ;
						dcterms:creator <https://orcid.org/0000-0002-7809-9798> ;
						dcterms:creator <https://orcid.org/0000-0003-4894-6965> ;
						dcterms:creator <https://orcid.org/0000-0001-8526-8847> ;
						dcterms:license <https://creativecommons.org/licenses/by/1.0/> ;
						vann:preferredNamespacePrefix    "geomapi" ;
						vann:preferredNamespaceUri <https://w3id.org/geomapi#> .
						owl:imports <https://w3id.org/fog#>


#################################################################
#    DATA FORMATS
###################################################################

#Extend FOG data formats in geomapi
	#IMG_EXTENSION = [".jpg", ".png", ".JPG", ".PNG", ".JPEG"]
	#MESH_EXTENSION = [".obj",".ply",".fbx" ]
	#PCD_EXTENSION = [".pcd", ".e57",".pts", ".ply", '.xml','.csv']

#CAD formats -> mostly covered by fog
geomapi:asDxF
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  dbp:extension ".dxf" ;
  rdfs:comment "The datatype relation pointing to a geometry description following the proprietary DXF format. It consists of a single .dxf file (binary)."@en ;
  rdfs:isDefinedBy geomapi: ;
  rdfs:label "as DXF format (.dxf)"@en ;
  rdfs:subPropertyOf fog:hasSimpleGeometryDescription ;

#BIM formats -> ifc is in fog

#MESH formats # -> this is to specific in fog and clutters the geometry parsing using the API's
geomapi:asPolygonalMesh
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  dbp:extension ".obj" ;
  dbp:extension ".ply" ;
  dbp:extension ".fbx" ;
  rdfs:comment "Polygonal mesh archetype (binary or ASCII) that can be parsed with Open3D or Trimesh."@en ;
  rdfs:isDefinedBy geomapi: ;
  rdfs:label "as polygonal mesh"@en ;
  rdfs:subPropertyOf fog:hasSimpleGeometryDescription ; 

#POINT CLOUD formats
geomapi:asPointCloud
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  dbp:extension ".pcd" ;
  dbp:extension ".e57" ;
  dbp:extension ".las" ;
  dbp:extension ".laz" ;
  dbp:extension ".pts" ;
  dbp:extension ".ply" ;
  dbp:extension ".xml" ;
  dbp:extension ".csv" ;
  dbp:extension ".bin" ;
  rdfs:comment "Point cloud formats (binary or ASCII) that can be parsed with Python Laspy, Pye57, Pandas or Open3D."@en ;
  rdfs:isDefinedBy geomapi: ;
  rdfs:label "as point cloud"@en ;
  rdfs:subPropertyOf fog:hasSimpleGeometryDescription ;  

#IMAGE formats
geomapi:asImage
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  dbp:extension ".jpeg" ;
  dbp:extension ".jpg" ;
  dbp:extension ".png" ;
  rdfs:comment "Image archetype that can be parsed by Python PIL or OpenCV."@en ;
  rdfs:isDefinedBy geomapi: ;
  rdfs:label "as image"@en ;
  rdfs:subPropertyOf fog:hasSimpleGeometryDescription ;

geomapi:asTexture
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  dcterms:requires "can i fill in a method here????????" ;
  rdfs:comment "A geometry texture file. This file always has to be used together with a geometry resource that references it."@en ;
  rdfs:isDefinedBy geomapi: ;
  rdfs:label "as texture file"@en ;
  rdfs:subPropertyOf geomapi:asImage ;

geomapi:asPanoramicImage
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  dcterms:requires "can i fill in a method here????????" ;
  rdfs:comment "A panoramic image captured from a sensor or stiched together in software."@en ;
  rdfs:isDefinedBy geomapi: ;
  rdfs:label "as Panoramic image"@en ;
  rdfs:subPropertyOf geomapi:asImage ; 

geomapi:asOrthomosaic
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  dcterms:requires "can i fill in a method here????????" ;
  rdfs:comment "Orhotographic composition of undistorted images as produced by a photogrammetric process."@en ;
  rdfs:isDefinedBy geomapi: ;
  rdfs:label "as Orthomosaic"@en ;
  rdfs:subPropertyOf geomapi:asImage ;



#################################################################
#    MetaData definitions
##################################################################

geomapi:GeomapiMetaDataProperty rdf:type owl:AnnotationProperty ;
                                <http://purl.org/dc/elements/1.1/description> "Geometric annotation property added querying purposes." .


geomapi:cartesianBounds a owl:DatatypeProperty ; #annotation because otherwise it has to point to another URI, cluttering the repr.
	rdfs:comment """The {X_min,X_max,Y_min,Y_max,Z_min,Z_max} bounds of a geometry.""" ;
	rdfs:label "Cartesian Boundaries" ;
	rdfs:subClassOf owl:Thing ;
	rdfs:subPropertyOf :GeomapiMetaDataProperty ;
	
  rdfs:domain bot:Element ;
  rdfs:range bot:Element ;
  vs:term_status "stable" ;
  rdfs:isDefinedBy bot: .

<http://www.w3.org/2003/01/geo/wgs84_pos#alt> rdf:type owl:AnnotationProperty ;
                                              rdfs:comment """The WGS84 altitude of a SpatialThing (decimal  above the local reference ellipsoid).""" ;
                                              rdfs:label "altitude" ;
                                              rdfs:subPropertyOf :GeomapiMetaDataProperty ;
                                              rdfs:domain <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> .



