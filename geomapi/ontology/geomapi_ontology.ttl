###  https://w3id.org/geomapi#matrix
:matrix 
	rdf:type rdfs:Datatype ;
	rdfs:comment "a python compatible matrix i.e. a 2 X 2 matrix M: \"[m11,m21,m12,m22]\"^^geomapi:matrix"@en ;
    rdfs:isDefinedBy : ;
    rdfs:seeAlso gom:rowMajorArray;
    :method "geomapi.utils.literal_to_matrix";
    rdfs:label "Matrix"@en .

xsd:dateTime
	:method "geomapi.utils.validate_timestamp" .
	
xsd:float
	:method "geomapi.utils.literal_to_float" .

gom:rowMajorArray
	:method "geomapi.utils.literal_to_matrix".

